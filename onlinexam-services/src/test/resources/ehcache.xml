<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />
    <cache name="com.chemcyber.ichemsoft.domain.AlarmType"
    		timeToLiveSeconds="3600">
    </cache>

    <cache name="cn.edu.nju.dzy.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="cn.edu.nju.dzy.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="cn.edu.nju.dzy.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="cn.edu.nju.dzy.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="cn.edu.nju.dzy.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.chemcyber.ichemsoft.domain.PersistentAuditEvent"
           timeToLiveSeconds="3600">
    </cache>
</ehcache>

